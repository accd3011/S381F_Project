<html>
    <body>
        <H2>Welcome <%= username %> Restaurant (Find <%= nRestaurants %> records)</H2>
        <ul>
            <% for (var r of restaurant) { %>
            <li>Restaurants Name: <a href="/details?_id=<%= r._id %>"><%=r.name %></a></li>
            <% } %>
        </ul>
        <form action="/insert" method="get">
            <input type="submit" value="Create new records">
        </form>
        <form action="/search" method="get">
            <input type="submit" value="Search records">
        </form>
        <form action="/logout" method="get">
            <input type="submit" value="Logout here">

        </form>
    </body>
</html>
